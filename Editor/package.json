{
  "name": "taxxoreditor",
  "version": "1.0.0",
  "description": "Application for editing filings in the Taxxor ecosystem",
  "main": "index.js",
  "scripts": {
    "start-local": "lsof -n -i4TCP:4812 | grep LISTEN | tr -s ' ' | cut -f 2 -d ' ' | xargs kill -9 & gulp dotnet & dotnet watch run --project TaxxorEditor",
    "start": "gulp dotnet && [ -n \"$TAXXOR_DEV_ENVIRONMENT\" ] && [ \"$TAXXOR_DEV_ENVIRONMENT\" = \"devcontainer-server\" ] && dotnet watch run --project TaxxorEditor || docker logs -f --tail 500 tdm-editor-1",
    "start-sdk": "dotnet watch run --verbose --no-hot-reload",
    "start-docker": "docker logs -f tdm-editor-1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/taxxor/taxxoreditor.git"
  },
  "author": "Johan Thijs",
  "license": "MIT",
  "homepage": "https://bitbucket.org/taxxor/taxxoreditor#readme",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@syncfusion/ej2": "^18.4.31",
    "@types/bootstrap": "^4.3.1",
    "@types/es6-promise": "^3.3.0",
    "@types/jquery": "^3.5.7",
    "@types/jquery.contextmenu": "^1.7.34",
    "@types/node": "^13.13.0",
    "@types/tinymce": "^4.6.4",
    "bootstrap": "^4.3.1",
    "gulp": "^5.0.0",
    "html5sortable": "^0.9.16",
    "jquery": "^3.6.0",
    "jquery-contextmenu": "^3.0.0-beta.2",
    "js-cookie": "^2.2.1",
    "natives": "^1.1.6",
    "typescript": "^4.4.4",
    "xmldom": "^0.6.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%"
  ]
}
