                                                                <option value="textandtables">Text &amp; Tables</option>
                                                                <option value="tables">Tables only</option>
                                                                <option value="text">Text only</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </label>
                                    </div>
                                </div>
                            </div>

