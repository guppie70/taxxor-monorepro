<?xml version="1.0"?>
<configuration>
  <!-- generic information -->
  <general>
    <site_identifier>Taxxor Editor</site_identifier>
    <application_id>taxxoreditor</application_id>
    <default_document></default_document>

    <settings>
      <setting id="localization_mechanism">domain</setting>
      <setting id="development_environment">
        <domain type="local" />
      </setting>
      <setting id="lextra_api_key">872ebcf4-1a93-4a26-9786-a91b243036a7</setting>
      <!-- set this to true to avoid needing to login all the time during development -->
      <setting id="bypass-security">false</setting>
      <setting id="enable-cross-site-request-forgery-check">false</setting>


      <!-- Static files webserver settings -->
      <setting id="serve-static-files">true</setting>
      <setting id="static-files-require-authentication">true</setting>
      <setting id="staticfiles-bypass-regex">^(.*\.ico|.*\.map|\/stylesheets\/\w+\.css.*|\/styles\/\w+\.css.*|\/javascript\/(\w|\-)+\.js|\/scripts\/(\w|\-)+\.(js|css|htm).*|\/custom\/.*|.*\.properties|.*\/fonts\/.*|\/images\/.*|\/static\/.*)$</setting>

      <!-- logging and debugging settings -->
      <setting id="console-dump-requestvariables">false</setting>
      <setting id="console-dump-projectvariables">false</setting> 
      <setting id="store-inspector-files">true</setting>
      <setting id="debug-redirects">false</setting>
      <setting id="log-uris">true</setting>

      <setting id="sessioncookies">
        <name type="applicationsession">sessionidtxeditor</name>
      </setting>

      <!-- Double session detection -->
      <setting id="double_session_detection" timeout="30">
        <!--
            <domain type="local" />
            <domain type="dev" />
        -->
      </setting>

      <!-- HTTP response headers -->
      <setting id="header_application_version">

      </setting>

